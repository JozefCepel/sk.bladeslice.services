      <wsdl:fault name="LokalneRegistreFault_Exception" message="tns:LokalneRegistreFault_Exception" />
    </wsdl:operation>
    <wsdl:operation name="SearchSpecialTownStreets">
      <wsdl:documentation>Sluzba vracia zoznam nazvov ulic (deduplikovany, DISTINCT) pre specialne pripady miest zlozenych z viacerych obci (Bratislava/Kosice). Vyhladavacie kriteria maju mat vyplnene osobitneMesto a ulica (prefix).</wsdl:documentation>
      <wsdl:input name="SearchSpecialTownStreets" message="tns:SearchSpecialTownStreets" />
      <wsdl:output name="SearchSpecialTownStreetsResponse" message="tns:SearchSpecialTownStreetsResponse" />
      <wsdl:fault name="LokalneRegistreFault_Exception" message="tns:LokalneRegistreFault_Exception" />
