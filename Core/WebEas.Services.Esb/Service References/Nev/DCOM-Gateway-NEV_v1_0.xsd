      <xs:element minOccurs="1" maxOccurs="1" name="VehicleID" type="xs:int" />
      <xs:element minOccurs="0" maxOccurs="1" name="RecordID" type="xs:long">
        <xs:annotation>
          <xs:documentation>Číslo záznamu v epodatelni, vracia sa len v prípade volania v kontexte úradníka, ak bol zadaný vstupný element DossierID</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="Owner" type="nev:Person" />
      <xs:element minOccurs="1" maxOccurs="1" name="GeneralInfo" type="nev:GeneralInfo" />
      <xs:element minOccurs="1" maxOccurs="1" name="Holder" type="nev:Person" />
      <xs:element minOccurs="1" maxOccurs="1" name="TechnicalInfo" type="nev:TechnicalInfo" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="Person" type="nev:Person" />
