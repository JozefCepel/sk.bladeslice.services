<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:nev="http://schemas.dcom.sk/integration/nev/1.0" elementFormDefault="qualified" targetNamespace="http://schemas.dcom.sk/integration/nev/1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
