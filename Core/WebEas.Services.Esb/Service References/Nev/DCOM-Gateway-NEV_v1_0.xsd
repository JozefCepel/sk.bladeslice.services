      <xs:element minOccurs="0" maxOccurs="1" name="LastName" type="xs:string" />
      <xs:element minOccurs="0" maxOccurs="1" name="Nationality" nillable="true" type="xs:int" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="LegalPerson" type="nev:LegalPerson" />
  <xs:complexType name="LegalPerson">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="1" name="Name" type="xs:string" />
      <xs:element minOccurs="0" maxOccurs="1" name="OrganizationID" type="xs:string" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="GetVehicleInfoRes" type="nev:VehicleInfo" />
  <xs:element name="GetVehicleInfoReq" type="nev:SearchCriteria" />
</xs:schema>