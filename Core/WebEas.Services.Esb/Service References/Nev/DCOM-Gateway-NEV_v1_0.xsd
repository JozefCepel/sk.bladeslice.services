      <xs:element minOccurs="0" maxOccurs="1" name="EngineRotationSpeed" nillable="true" type="xs:int" />
      <xs:element minOccurs="0" maxOccurs="1" name="EngineNumber" type="xs:string" />
      <xs:element minOccurs="0" maxOccurs="1" name="GearsNumber" nillable="true" type="xs:int" />
      <xs:element minOccurs="0" maxOccurs="1" name="BodyShellType" type="xs:string" />
      <xs:element minOccurs="0" maxOccurs="1" name="ColorPrimary" type="xs:string" />
