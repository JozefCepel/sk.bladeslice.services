      <xs:element minOccurs="0" maxOccurs="1" name="VehicleIdentificationCardIssuer" type="xs:string" />
      <xs:element minOccurs="0" maxOccurs="1" name="VehicleIdentificationCardInvalid" nillable="true" type="xs:boolean" />
